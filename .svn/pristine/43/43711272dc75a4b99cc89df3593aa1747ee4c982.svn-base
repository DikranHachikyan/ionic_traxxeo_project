<ion-content padding>
    <ion-navbar>
      <ion-title padding>Settings</ion-title>
    </ion-navbar>
  <ion-list>

    <ion-item>
      <ion-thumbnail>
          <ion-img width="auto" height="auto" class="img-settings" src="../../assets/img/user.png"></ion-img>
      </ion-thumbnail>
    </ion-item>

    <ion-item>
      <ion-label>Person</ion-label>
      <ion-checkbox color="primary" checked="true"></ion-checkbox>
    </ion-item>

    <ion-item>
      <ion-label>Object</ion-label>
      <ion-checkbox color="primary" checked="true"></ion-checkbox>
    </ion-item>

    <ion-item>
      <ion-label>Vehicle</ion-label>
      <ion-checkbox color="primary" checked="true"></ion-checkbox>
    </ion-item>

  </ion-list>

  <button ion-button small full color="default">Apply Settings</button>
</ion-content>
