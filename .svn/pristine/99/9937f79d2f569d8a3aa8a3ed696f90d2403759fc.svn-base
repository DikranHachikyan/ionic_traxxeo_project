<!--
  Generated template for the SettingsPage page.

  See http://ionicframework.com/docs/components/#navigation for more info on
  Ionic pages and navigation.
-->

<ion-content padding>
 <my-header></my-header>
  <ion-navbar padding>
    <ion-title>Settings</ion-title>
  </ion-navbar>

  <ion-list >
    <ion-item>
      <ion-thumbnail>
          <ion-img width="auto" height="auto" class="img-settings" src="assets/img/user.png"></ion-img>
        </ion-thumbnail>
    </ion-item>
    <ion-item>
      <ion-label>Person</ion-label>
      <ion-checkbox color="primary" [(ngModel)]="checkedPerson"></ion-checkbox>
    </ion-item>
    <ion-item>
      <ion-label>Object</ion-label>
      <ion-checkbox color="primary" [(ngModel)]="checkedObject"></ion-checkbox>
    </ion-item>
    <ion-item>
      <ion-label>Vehicle</ion-label>
      <ion-checkbox color="primary" [(ngModel)]="checkedVehicle"></ion-checkbox>
    </ion-item>

  </ion-list>

  <button small full color="default" (click)="gotoListPage()" name="ListPage" >Apply Settings</button>
</ion-content>
